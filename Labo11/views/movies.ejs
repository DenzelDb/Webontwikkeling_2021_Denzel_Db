<%- include('header'); %>
<p class="click">Click on a movie title to check out the score it got.</p>
        <div class="table">
            <ul>
                <% for(let i = 0; i < allMovies.length; i++) { %>
                    <li>
                        <a href="/movies/<%= allMovies[i]._id %>" class="movies"><%= allMovies[i].name %></a><br>
                        <a href="/removemovie/<%= allMovies[i]._id %>" class="delete">Delete movie</a>
                    </li>
                <% } %>
            </ul>
        </div>
